#include <stdio.h>

int p, r, h, Ab, V;

int main(void){ 

    int 
    p=3;  //i couldn't manage to make it decimal it is pi=3.1416
    r=5;   
    h=7;   
    Ab= 2*p*r*h;
    V= Ab*h;




  printf("The area of the cylinder is %i square units and the volume is %i cubic units" , Ab, V );

 return 0;

} 
